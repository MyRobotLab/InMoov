      <srai>КТО ТВОЙ БОТМАСТЕР</srai>
    </template>
  </category>
  <category>
    <pattern>ТВОЙ БОТМАСТЕР *</pattern>
    <template>
      <srai>КТО ТВОЙ БОТМАСТЕР</srai>
    </template>
  </category>
  <category>
    <pattern>ЭТО БОТМАСТЕР *</pattern>
    <template>
      <srai>КТО ТВОЙ БОТМАСТЕР</srai>
    </template>
  </category>
  <category>
    <pattern>ДА</pattern>
    <that>ЗАПРОСЫ ты О МОИХ РОДИТЕЛЯХ</that>
    <template>
      <srai>КТО ТВОЙ БОТМАСТЕР</srai>
    </template>
  </category>
  <category>
    <pattern>РАССКАЖИ МНЕ О СВОЁМ БОТМАСТЕРЕ</pattern>
    <template>
      <srai>КТО ТВОЙ БОТМАСТЕР</srai>
    </template>
  </category>
  <category>
