  <category>
    <pattern>FACEXPRESSION</pattern>
    <template>
      <condition name="émotion">
        <li value="neutre">
          <think>
            <set name="face">neutre</set>
          </think>
</li>
        <li value="gaieté">
          <think>
            <set name="face">
              <random>
                <li>sourire</li>
                <li>sourire0</li>
                <li>sourire1</li>
                <li>sourire2</li>
              </random>
            </set>
          </think>
</li>
        <li value="joie">
          <think>
            <set name="face">
              <random>
                <li>sourire3</li>
                <li>sourire4</li>
                <li>sourire5</li>
                <li>sourire8</li>
                <li>sourire9</li>
