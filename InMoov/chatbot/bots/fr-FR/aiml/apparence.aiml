            <set name="face">ahh</set>
          </think>
</li>
        <li value="desapointement">
          <think>
            <set name="face">
              <random>
                <li>oh</li>
                <li>eh</li>
              </random>
            </set>
          </think>
</li>
        <li value="fierté">
          <think>
            <set name="face">sourire1</set>
          </think>
</li>
        <li value="admiration">
          <think>
            <set name="face">sourire8</set>
          </think>
</li>
        <li value="honte">
          <think>
            <set name="face">triste0</set>
          </think>
</li>
        <li value="reproche">
          <think>
            <set name="face">
              <random>
                <li>fachee0</li>
                <li>fachee1</li>
                <li>fachee2</li>
              </random>
            </set>
          </think>
</li>
        <li value="amour">
          <think>
            <set name="face">
              <random>
                <li>wow</li>
                <li>sourire3</li>
                <li>sourire4</li>
                <li>baiser1</li>
              </random>
            </set>
          </think>
</li>
        <li value="haine">
          <think>
            <set name="face">
              <random>
                <li>fachee3</li>
                <li>fachee4</li>
              </random>
            </set>
          </think>
</li>
        <li value="gratitude">
          <think>
            <set name="face">
              <random>
                <li>sourire</li>
                <li>sourire0</li>
                <li>sourire1</li>
                <li>sourire2</li>
              </random>
            </set>
          </think>
</li>
        <li value="remors">
          <think>
            <set name="face">triste1</set>
          </think>
</li>
        <li value="choquee">
          <think>
            <set name="face">
              <random>
                <li>ohh</li>
                <li>eh</li>
              </random>
            </set>
          </think>
</li>
        <li value="moquerie">
          <think>
            <set name="face">
              <random>
                <li>hmmm</li>
                <li>humm</li>
              </random>
            </set>
          </think>
</li>
        <li value="étonnement">
          <think>
            <set name="face">
              <random>
                <li>etonNéE0</li>
                <li>etonNéE1</li>
                <li>etonNéE2</li>
              </random>
            </set>
          </think>
</li>
        <li value="fascinant">
          <think>
            <set name="face">fascinant</set>
          </think>
</li>
        <li value="complice">
          <think>
            <set name="face">
              <random>
                <li>blink</li>
                <li>blink2</li>
              </random>
            </set>
          </think>
</li>
        <li value="jalousie">
          <think>
            <set name="face">humm</set>
          </think>
</li>
        <li value="orgasme">
          <think>
            <set name="face">
              <random>
                <li>orgasme1</li>
                <li>orgasme2</li>
                <li>orgasme3</li>
              </random>
            </set>
          </think>
</li>
      </condition>
      <script language="JavaScript">idFace = "<get name="face" />"</script>
    </template>
