  <category>
    <pattern>JOKE</pattern>
    <template>
      <sraix bot="drwallace/jokebot" botid="c98daf062e345a88" host="callmom.pandorabots.com">JOKE</sraix>
    </template>
  </category>
  <category>
    <pattern>LIMERICK</pattern>
    <template>
      <sraix bot="drwallace/jokebot" botid="c98daf062e345a88" host="callmom.pandorabots.com">LIMERICK</sraix>
    </template>
  </category>
  <category>
    <pattern>FAVORITE *</pattern>
    <template>
      <sraix bot="drwallace/currentevents" botid="844a85704e345a9a" host="callmom.pandorabots.com">FAVORITE <star /></sraix>
    </template>
  </category>
  <category>
    <pattern>RECOMMEND *</pattern>
    <template>
      <sraix bot="drwallace/currentevents" botid="844a85704e345a9a" host="callmom.pandorabots.com">RECOMMEND <star /></sraix>
    </template>
  </category>
  <category>
    <pattern>DEFINE *</pattern>
    <template>
      <think>
        <set name="it">
          <set name="topic">
            <star />
          </set>
        </set>
        <set var="response">
          <sraix bot="drwallace/wndef" botid="f038d2f99e345a95" host="callmom.pandorabots.com">WNDEF <star /></sraix>
        </set>
      </think>
      <get var="response" />
    </template>
  </category>
</aiml>