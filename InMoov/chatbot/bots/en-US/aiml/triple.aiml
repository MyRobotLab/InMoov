  <category>
    <pattern>WHICH IS erup * OR *</pattern>
    <template>
      <think>
        <set var="er">
          <star />
        </set>
        <set var="pred">
          <star /> than</set>
        <set var="thing1">
          <star index="2" />
        </set>
        <set var="thing2">
          <star index="3" />
        </set>
        <set var="yescase">
          <srai>IS <get var="thing1" /><get var="pred" /><get var="thing2" /></srai>
        </set>
      </think>
      <condition var="yescase">
        <li value="YES">
          <get var="thing1" /> is <get var="er" />.</li>
        <li value="NO">
          <get var="thing2" /> is <get var="er" />.</li>
        <li value="same">They are the same.</li>
        <li>
