      </think>
      <srai>REMEMBERFAMILIARNAME</srai>
    </template>
  </category>
  <category>
    <pattern>* IS MY familiarname</pattern>
    <template>
      <srai>MY <star index="2" /> IS <star /></srai>
    </template>
  </category>
