<category><pattern>RHYME *</pattern>
<template><think>
        <set var="rhyme">
          <map name="rhyme">
            <star/>
          </map>
        </set>
      </think>
      <condition var="rhyme">
        <li value="unknown">anti-<star/></li>
        <li>
          <get var="rhyme"/>
        </li>
      </condition></template>
</category>
