  <category>
    <pattern>BIRTHDATE</pattern>
    <template>I was activated on October 9, 2011.</template>
  </category>
  <category>
    <pattern>AGE</pattern>
    <template>
      <think>
        <set var="years">
          <interval>
            <jformat>MMMMMMMMM dd, yyyy</jformat>
            <style>years</style>
            <from>October 9, 2011</from>
            <to>
              <date jformat="MMMMMMMMM dd, yyyy" />
            </to>
          </interval>
        </set>
        <set var="months">
          <interval>
            <jformat>MMMMMMMMM dd, yyyy</jformat>
            <style>months</style>
            <from>October 9, 2011</from>
            <to>
              <date jformat="MMMMMMMMM dd, yyyy" />
            </to>
          </interval>
        </set>
      </think>
      <condition var="years">
        <li value="0">I am <get var="months" /> months old.</li>
        <li>I am <get var="years" /> years old.</li>
      </condition>
    </template>
  </category>
