<!-- météo -->
  
  <category>
    <pattern># MÉTÉO</pattern>
    <template>
      <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny()</param>
        </mrl>
      </oob>
    </template>
  </category>
  
  <category>
    <pattern># est ce # <set>weather</set> #</pattern>
    <template>
      <srai>MÉTÉO</srai>
    </template>
  </category>
  
  <category>
    <pattern># est ce # <set>weather</set> $DEMAIN</pattern>
    <template>
      <srai>QUEL TEMPS # DEMAIN</srai>
    </template>
  </category>
  
  <category>
  <pattern>^ va t il ^ <set>weather</set> ^</pattern>
    <template>
      <srai>MÉTÉO</srai>
    </template>
  </category>
  
  
  <category>
    <pattern>QUEL TEMPS *</pattern>
    <template>
      <srai># MÉTÉO</srai>
    </template>
  </category>
  
  <category>
    <pattern>QUEL TEMPS # DEMAIN</pattern>
    <template>
       <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny(town,8)</param>
        </mrl>
      </oob>
    </template>
  </category>
  
  <category>
    <pattern>QUEL TEMPS ^ DANS * JOURS</pattern>
    <template>
        <think><set name="jours"><map name="name2number"><star index="2" /></map></set></think>
        <condition>
            <li name="jours" value="unknown"><think><set name="jours"><star index="2" /></set></think></li>
        </condition>
       <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny(town,
<javascript>
period = eval( <get name="jours" /> * 8 );
period;
</javascript>)
</param>
        </mrl>
      </oob>
    </template>
  </category>
  
    <category>
    <pattern>QUEL TEMPS # À * DANS * JOURS</pattern>
    <template>
    <think><set name="jours"><map name="name2number"><star index="3" /></map></set></think>
    <condition>
        <li name="jours" value="unknown"><think><set name="jours"><star index="3" /></set></think></li>
    </condition>
        
       <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny("<star index="2" />",<javascript>
period = eval( <get name="jours" /> * 8 );
period;
</javascript>)</param>
        </mrl>
      </oob>
    </template>
  </category>
  
    <category>
    <pattern>QUEL TEMPS ^ DEMAIN À *</pattern>
    <template>
       <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny("<star index="2" />",8)</param>
        </mrl>
      </oob>
    </template>
  </category>
  
  <category>
    <pattern>QUEL TEMPS FAIT IL À *</pattern>
    <template>
      <srai># MÉTÉO à <star /></srai>
    </template>
  </category>
  <category>
    <pattern>QUEL TEMPS FAIT IL SUR *</pattern>
    <template>
      <srai># MÉTÉO à <star /></srai>
    </template>
  </category>
  
  <category>
    <pattern># MÉTÉO À *</pattern>
    <template>
      <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny("<star index="2" />")</param>
        </mrl>
      </oob>
    </template>
  </category>
  
  <category>
    <pattern>SYSTEM openweathermapError</pattern>
    <template>
      Erreur de clé a.p.i. ou cette ville est inconnue
    </template>
  </category>
  
