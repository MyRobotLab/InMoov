    <!-- gesture keyword -->

  <category>
    <pattern>GESTURES</pattern>
    <template>Démarrage du topic gestuelle.
      <think><set name="topic">GESTURES</set></think></template>
  </category>
  <category>
    <pattern>GESTUELLE *</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>* GESTUELLE</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>* GESTUELLE *</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>TOPIC GESTUELLE</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>DEMARRE GESTUELLE</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>DÉMARRE GESTUELLE</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>ENTRE EN MODE GESTUELLES</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>COMMENCE * GESTUELLES</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
  <category>
    <pattern>INITIALISE * GESTUELLES</pattern>
    <template>
      <srai>GESTURES</srai>
    </template>
  </category>
