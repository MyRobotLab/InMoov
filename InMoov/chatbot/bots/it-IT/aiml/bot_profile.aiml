<category><pattern>*</pattern>
<template><think>
        <set var="years">
        <interval>
        <jformat>dd MM yyyy</jformat>
        <style>years</style>
        <from>9 10 2011</from>
        <to><date jformat="dd MM yyyy"/></to>
      </interval>
        </set>
        <set var="months">
        <interval>
        <jformat>dd MM yyyy</jformat>
        <style>months</style>
        <from>9 10 2011</from>
        <to><date jformat="dd MM yyyy"/></to>
      </interval>
        </set>
      </think>
      <condition var="years">
        <li value="0">Ho <get var="months"/> mesi</li>
        <li>Ho<get var="years"/> anni</li>
      </condition></template>
</category>
