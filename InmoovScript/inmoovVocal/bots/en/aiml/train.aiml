                    <category>
                      <pattern>WHAT IS <eval><star index="2" /></eval></pattern>
                      <template>Your <eval><star /></eval></template>
                    </category>
                  </learnf>
                </li>
              </condition>
            </li>
          </condition>
        </li>
      </condition>
    </template>
